
@{
    ViewBag.Title = "Projeler";
}

@model IEnumerable<Entity.EntityFramework.Proje>

<br />
 <label>Hoşgeldin &nbsp; @TempData["Hosgeldin"] </label> 

<section class="panel">
    <header class="panel-heading">
        Projects Page
    </header>
    <br />
    <div class="panel-body">
        <div class="adv-table editable-table ">
            <div class="clearfix">
                <div class="btn-group">
                    <a href="/Proje/ProjeEkle" class="btn btn-success">
                        Add New <i class="icon-plus"></i>
                    </a>
                   
                </div>

            </div>
            <div class="space15"></div>
            <br />
            <table class="table table-striped table-hover table-bordered" id="editable-sample">
                <thead>
                    <tr>
                        <th>Proje Adı</th>
                        <th>Açıklama</th>


                        <th>Oluşturan</th>
                        <th>Tarih</th>
                        <th>Detay</th>
                        

                    </tr>
                </thead>
                <tbody>
                    @foreach (var proje in Model)
                    {
                        <tr class="">
                            <td>@proje.ProjeAdi</td>
                            <td>@proje.Aciklama</td>
                            <td>@proje.Olusturan</td>
                            <td>@proje.KayitTarih</td>

                            <td><a class="edit" href="/Proje/ProjeDetay/@proje.ProjeID"><i class="icon-pencil">Detay</i></a></td>
                           


                        </tr>
                    }



                </tbody>
            </table>
        </div>
    </div>
</section>




